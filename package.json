{
  "name": "myApp",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:ios": "ionic cap run ios -l --external",
    "dev:android": "ionic cap run android -l --external",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:ui": "vitest --ui --coverage.enabled=true",
    "test:coverage": "vitest run --coverage --run",
    "test:e2e": "cypress run",
    "lint": "eslint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@capacitor/android": "5.5.1",
    "@capacitor/app": "5.0.6",
    "@capacitor/core": "5.5.1",
    "@capacitor/device": "^5.0.6",
    "@capacitor/haptics": "5.0.6",
    "@capacitor/ios": "5.5.1",
    "@capacitor/keyboard": "5.0.6",
    "@capacitor/status-bar": "5.0.6",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "eslint-plugin-prettier": "^5.0.1",
    "framer-motion": "^10.16.4",
    "install": "^0.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.11.0",
    "react-router": "^6.18.0",
    "react-router-dom": "^6.18.0",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@capacitor/cli": "5.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-helmet": "^6.1.11",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-react": "^4.0.1",
    "@vitest/coverage-istanbul": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "cypress": "^12.7.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jsdom": "^22.1.0",
    "prettier": "3.0.3",
    "sass": "^1.69.5",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6",
    "husky": "^8.0.0"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint",
      "prettier --write",
      "bash -c tsc -p .",
      "vitest run --coverage --run"
    ]
  },
  "description": "React application which can run the browser and on native devices"
}